@model IEnumerable<E_salesperson.Models.CommissionT>

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>

<table class="table">
    <th>
        <div>
            @Html.ActionLink("Back", "AllSalesPerson", "Admin")
        </div>
    </th>
    <th>
        <div style="text-align:right">
            @Html.ActionLink("Logout", "Login", "Login")
        </div>
    </th>

</table>



<table class="table">
    <tr>
        <th>
