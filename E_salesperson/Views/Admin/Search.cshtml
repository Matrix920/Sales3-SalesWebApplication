        <th>
            @Html.DisplayNameFor(model => model.CommTotal)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SalesPersonT.SalesPersonNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SalesPersonT.SalesPersonName)
